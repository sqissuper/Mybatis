<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatisee.dao.UserInfoMapper">
    <insert id="addUser" parameterType="com.example.demo.mybatisee.model.UserInfo">
        insert into userinfo(username,password)
        values(
            #{username},#{password}
        )
    </insert>

</mapper>